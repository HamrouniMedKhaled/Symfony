{% extends 'default/layout.html.twig' %}

{% block banner %}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <style>
        #map {
            height: 400px;
            width: 100%;
        }
    </style>
{% endblock %}

{% block content %}
    <h1>Modifier Enseigne</h1>
    {{ form_start(form) }}
    <table>
        <tr>
            <td>nom</td>
            <td>{{ form_widget(form.nom) }}</td>

        </tr>
        <tr>
            <td>Date Mise en circulation</td>
            <td>{{ form_widget(form.description) }}</td>

        </tr>
        <tr>
            <td>marque</td>
            <td>{{ form_widget(form.adresse) }}</td>

        </tr>
        <tr>
            <td>modele</td>
            <td>{{ form_widget(form.image) }}</td>
        </tr>
        <td></td>
        <td>{{ form_widget(form.enregistrer) }}</td>
        </tr>

    </table>
    {{ form_end(form) }}

{% endblock %}